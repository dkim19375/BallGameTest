<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<!--suppress ALL -->
<VBox alignment="CENTER" prefHeight="553.0" prefWidth="820.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Profiles">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <ListView fx:id="profileList" prefWidth="405.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="18.0" prefWidth="92.0" text="Password:">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <PasswordField fx:id="passwordField" prefHeight="23.0" prefWidth="243.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="64.0" prefWidth="820.0">
         <children>
            <HBox alignment="CENTER" prefWidth="250.0">
               <children>
                  <Button fx:id="createButton" mnemonicParsing="false" prefHeight="52.0" prefWidth="206.0" text="Create Profile">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefWidth="250.0">
               <children>
                  <Button fx:id="deleteButton" mnemonicParsing="false" prefHeight="52.0" prefWidth="206.0" text="Delete Profile">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="BOTTOM_CENTER" prefHeight="58.0" prefWidth="820.0">
         <children>
            <HBox alignment="CENTER" prefWidth="250.0">
               <children>
                  <Button fx:id="backButton" alignment="CENTER" mnemonicParsing="false" prefHeight="52.0" prefWidth="206.0" text="Back">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefWidth="250.0">
               <children>
                  <Button fx:id="selectButton" alignment="CENTER" mnemonicParsing="false" prefHeight="52.0" prefWidth="206.0" text="Select">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="67.0" prefWidth="820.0">
         <children>
            <Label fx:id="errorLabel" text="Error">
               <font>
                  <Font size="29.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</VBox>